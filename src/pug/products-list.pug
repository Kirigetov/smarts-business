                .s-tabs__content.js-tabs-content
                  

                .s-tabs__content.js-tabs-content
                  .s-filters
                    .s-filter.s-filter_static
                      .s-filter__title 
                        span Производитель:
                      ul.s-filter__list
                        - var values = [ "Apple", "HTC", "Lenovo", "LG" ]
                        each item in values
                          li.s-filter__item
                            label.checkbox
                              input(type="checkbox")
                              span.checkbox__text= item

                      ul.s-filter__list.s-filter__list_hidden.js-toggle-block(data-toggle="brands")
                        - var values = [ "Nillkin", "Nokia", "ODOYO", "Ozaki" ]
                        each item in values
                          li.s-filter__item
                            label.checkbox
                              input(type="checkbox")
                              span.checkbox__text= item
                      .toggle.toggle_pad
                        a.toggle-more.js-toggle-btn(href="", data-toggle="brands", data-text="Показать меньше") Показать еще

                    .s-filter.s-filter_static
                      .s-filter__title
                        span Цена(грн.): 

                      .filter-slider.js-slider(data-min="0", data-max="8000", data-initial="[1000,5000]", data-units="грн")
                        .filter-slider__row
                          .input.filter-slider__input-field
                            label.filter-slider__label(for="input-with-keypress-0") от
                            input.filter-slider__input.js-slider-from(type="text" value="1000")
                          .input.filter-slider__input-field
                            label.filter-slider__label(for="input-with-keypress-1") до
                            input.filter-slider__input.js-slider-to(type="text" value="5000")
                        .range-slider__line.js-slider-line

                    .s-filter
                      .s-filter__title.js-nav-link 
                        span Наличие
                        span.s-filter__arrow.icon-chevron
                      ul.s-filter__list.js-navdrop
                        - var values = [ "В Днепре", "В Киеве", "только доступные"  ]
                        each item in values
                          li.s-filter__item
                            label.checkbox
                              input(type="checkbox")
                              span.checkbox__text= item
                                span.checkbox__tooltip
                                  span.checkbox__tooltip-find Найдено 132
                                  a(href="").checkbox__tooltip-link показать

