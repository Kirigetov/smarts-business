extends _layout

block title
      title Главная страница

block content

  .main-container
    .header-container
      .container
        //- begin header
        .header.js-header
          .header__section.header__section_left
            .header__logo.logo
              a(href='', title='')
                img(src='img/logo-business.png', alt='logo')
            .header__menu
              nav.nav
                .nav__item
                  a.nav__link.js-toggle-item(href='', title='') 
                    span.nav__link-img.icon-manager
                    span.nav__link-text ВАШ МЕНЕДЖЕР
                  .nav__dropdown.js-toggle-drop
