                      li.sub-nav__item
                        span.sub-nav__type Имя: 
                        span.sub-nav__data Греков Роман 
                      li.sub-nav__item
                        span.sub-nav__type Телефон: 
                        span.sub-nav__data 0669263171
                      li.sub-nav__item
                        span.sub-nav__type E-mail: 
                        span.sub-nav__data r.grekov@telemart.com.ua 
                      li.sub-nav__item
                        span.sub-nav__type Skype: 
                        span.sub-nav__data g.r.e.c.h.a 
                .nav__item
                  a.nav__link.js-toggle-item(href='', title='') 
                    span.nav__link-img.icon-dollar
                    span.nav__link-text ФИНАНСЫ
                  .nav__dropdown.js-toggle-drop
                    ul.sub-nav
                      li.sub-nav__item
                        span.sub-nav__type Курс USD: 
                        span.sub-nav__data.sub-nav__data_bold 26.80/26.80
                      li.sub-nav__item
                        span.sub-nav__type Сверка: 
                        a.sub-nav__link(href='', title='') скачать
